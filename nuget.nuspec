<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.2.8</version>
        <id>datatables.net-rowreorder-bs</id>
        <description>This is RowReorder for DataTables with styling for [Bootstrap](https://getbootstrap.com/docs/3.3/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-RowReorder-Bootstrap.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>RowReorder  Row Reordering  Bootstrap  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-rowreorder" version=">=1.2.7"/>
            <dependency id="datatables.net-bs" version=">=1.10.24"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/rowReorder.bootstrap.js" target="content\Scripts"/>
