        <file src="js/rowReorder.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/rowReorder.bootstrap.css" target="content\Content"/>
        <file src="css/rowReorder.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/rowReorder.bootstrap.min.css" target="content\Content"/>
        <file src="css/rowReorder.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/rowReorder.bootstrap.d.ts" target="content\Scripts"/>
        <file src="types/rowReorder.bootstrap.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

